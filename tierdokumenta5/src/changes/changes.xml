<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://maven.apache.org/changes/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
	<properties>
		<title>Änderungen</title>
		<author>Michael Binder</author>
	</properties>
	<body>
		<release version="1.0.5" date="29.01.2023" description="Helden-Software 5.5.3">
			<action type="update" dev="mb">Getestet mit Helden-Software 5.5.3. </action>
			<action type="update" dev="mb">Download URL geändert. Die Downloads liegen jetzt auf GitHub.</action>
		</release>
		<release version="1.0.4" date="07.02.2015" description="Helden-Software 5.4.1">
			<action type="update" dev="mb">Getestet mit Helden-Software 5.4.1. Die Schnittstelle der Helden-Software hat sich geändert. Daher ist diese Version nicht voll kompatibel zu früheren Versionen der Helden-Software.</action>
			<action type="update" dev="mb">Anpassungen bei den Auswahlen der Vorteile, Nachteile, etc</action>
		</release>
		<release version="1.0.3" date="30.12.2014" description="Helden-Software 5.4.0">
			<action type="add" dev="mb">Getestet mit Helden-Software 5.4.0; nicht voll kompatibel zu früheren Versionen</action>
			<action type="update" dev="mb">Korrektur für Tiere: Grund sind Änderungen der Schnittstelle in der Helden-Software 5.4.0</action>
		</release>
		<release version="1.0.2" date="05.01.2014" description="Helden-Software 5.3.1 und Zeilentrennung">
			<action type="add" dev="mb">Getestet mit Helden-Software 5.3.1</action>
			<action type="update" dev="mb">Zeilentrennung bei mehrzeiligen Feldern verbessert.</action>
			<action type="update" dev="mb">Vorteile mit einer Auswahl werden angezeigt</action>
			<action type="update" dev="mb">PDF: anderes Schriftformat (TrueType statt OpenType) um ein Speichern des PDF mit dem Acrobat Reader XI zu unterstützen.</action>
		</release>
		<release version="1.0.1" date="05.12.2013" description="Layoutkorrekturen">
			<action type="add" dev="mb">Projektwebseite erstellt</action>
			<action type="update" dev="mb">Layoutkorrekturen: Rahmen bei Rüstungszonen verschoben</action>
		</release>
		<release version="1.0.0" date="12.11.2013" description="Helden-Software 5.2.7">
			<action type="add" dev="mb">Anpassungen an die Helden-Software 5.2.7</action>
		</release>
		<release version="2013-03-02" date="02.03.2013" description="Helden-Software 5.2.6">
			<action type="add" dev="mb">Erste Version</action>
			<action type="add" dev="mb">Anpassungen an die Helden-Software 5.2.6</action>
		</release>
	</body>
